{
  "name": "svelte-fullscreen",
  "version": "1.1.1",
  "description": "Component that performs fullscreen in DOM Elements",
  "repository": "https://github.com/andrelmlins/svelte-fullscreen",
  "author": "Andr√© Lins <andrelucas01@hotmail.com> (https://andrelmlins.github.io/)",
  "license": "MIT",
  "svelte": "src/lib/Fullscreen.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "homepage": "https://svelte-fullscreen.netlify.com/",
  "files": [
    "src/lib/Fullscreen.svelte",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "screenfull": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^27.0.6",
    "jest": "^26.4.0",
    "rollup": "^2.26.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.32.0",
    "svelte-jester": "1.1.5"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src/lib",
    "coverage": "npm run test --coverage"
  },
  "bugs": {
    "url": "https://github.com/andrelmlins/svelte-fullscreen/issues"
