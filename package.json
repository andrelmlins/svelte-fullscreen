{
  "name": "svelte-fullscreen",
  "version": "2.0.0",
  "description": "Component that performs fullscreen in DOM Elements",
  "repository": "https://github.com/andrelmlins/svelte-fullscreen",
  "author": "Andr√© Lins <andrelucas01@hotmail.com> (https://andrelmlins.github.io/)",
  "license": "MIT",
  "svelte": "dist/Fullscreen.svelte",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://svelte-fullscreen.netlify.com/",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "screenfull": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "babel-jest": "^27.0.6",
    "jest": "^26.4.0",
    "rollup": "^2.26.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.1",
    "sirv-cli": "^1.0.6",
    "svelte": "^3.42.4",
    "svelte-dts": "^0.3.5",
    "svelte-jester": "1.1.5",
    "svelte-preprocess": "^4.8.0",
    "svelte-transpile-typescript": "^0.1.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "rollup -c && svelte-transpile-typescript -i src/lib/Fullscreen.svelte -o dist/Fullscreen.svelte",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest src/lib",
    "coverage": "npm run test --coverage"
  },
  "bugs": {
    "url": "https://github.com/andrelmlins/svelte-fullscreen/issues"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "keywords": [
    "svelte",
    "fullscreen"
  ]
}
